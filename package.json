{
  "name": "appsby",
  "version": "0.0.9",
  "description": "Everything you need to make a modern app with Gatsby. Write server functions alongside client code, with an easy and powerful React-style API. Views, search, uploads, all out of the box.",
  "main": "./public/js/client/index.js",
  "scripts": {
    "build": "webpack",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "files": [
    "./public/js/client/index.js",
    "./public/js/server/index.js"
  ],
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Audal Labs <e@audallabs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/audal/appsby.git",
    "directory": "/"
  },
  "homepage": "https://appsby.github.io",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "babel-core": "7.0.0-bridge.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/traverse": "^7.11.5",
    "@babel/preset-env": "^7.12.11",
    "babel-plugin-source-map-support": "^2.1.3",
    "babel-loader": "^8.2.2",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.3.0",
    "@aws-sdk/s3-presigned-post": "^3.3.0",
    "@aws-sdk/s3-request-presigner": "^3.3.0",
    "@fingerprintjs/fingerprintjs": "^3.0.0",
    "@tsmx/string-crypto": "^1.0.2",
    "cachios": "^2.2.5",
    "confusables": "^1.0.0",
    "faunadb": "^3.0.1",
    "ffmpeg-stream": "^0.7.0",
    "is-valid-username": "0.0.1",
    "js-base64": "^3.4.5",
    "jsonwebtoken": "^8.5.1",
    "magic-bytes.js": "^1.0.0",
    "nodemailer": "^6.4.17",
    "parse-domain": "^3.0.3",
    "react": "^17.0.1",
    "react-gstate": "^2.0.2",
    "rereadable-stream": "^1.4.3",
    "retry-axios": "^2.4.0",
    "s3-readable-stream": "0.0.9",
    "s3-upload-stream": "^1.0.7",
    "source-map-support": "^0.5.19",
    "streaming-s3": "^0.4.4",
    "string-sanitizer": "^1.1.1",
    "uuid": "^8.3.0",
    "file-type": "^16.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
